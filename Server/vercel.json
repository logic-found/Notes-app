{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": { "x-vercel-id": "*" },
            "continue": true,
            "status": 200,
            "timeout": 200
      
        }
    ]
}